{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@config": [ "./config" ],
      "@internal/*": [ "./internal/*" ],
      "@loaders/*": [ "./loaders/*" ],
      "@structs/*": [ "./structs/*" ]
    },
    "allowJs": true,
    "esModuleInterop": true,
    "noEmit": true,
    "strict": true
  },
  "include": [ "./src" ]
}